{
  "name": "bowenislandtattooshop",
  "version": "0.1.1",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "dev:frontend": "next dev --turbopack --port 3000",
    "dev:backend": "cross-env PORT=3001 tsx watch lib/server.ts",
    "dev:server": "cross-env PORT=3001 tsx watch lib/server.ts",
    "dev:frontend-wait": "wait-on http://localhost:3001/health && npm run dev:frontend",
    "dev:all": "concurrently --kill-others-on-fail --prefix-colors \"cyan.bold,magenta.bold\" --names \"BACKEND,FRONTEND\" \"npm run dev:backend\" \"npm run dev:frontend-wait\"",
    "dev:parallel": "start \"Backend\" npm run dev:backend && start \"Frontend\" npm run dev:frontend-wait",
    "build": "next build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:railway": "npx prisma generate && npm run build:server",
    "start": "next start",
    "start:server": "node dist/lib/server.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:server\"",
    "lint": "next lint",
    "test": "vitest",
    "cron:reminders": "tsx scripts/payment-reminder-cron.ts",
    "cron:reminders:test": "cross-env NODE_ENV=development tsx scripts/payment-reminder-cron.ts",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "setup:admin": "tsx scripts/setup-admin.ts",
    "seed:emails": "tsx scripts/seedEmailTemplates.ts",
    "test:email": "tsx scripts/testEmailFlow.ts",
    "test:email-api": "tsx scripts/testEmailApi.ts",
    "check:email-logs": "tsx scripts/checkEmailLogs.ts",
    "check:users": "tsx scripts/checkUserRole.ts",
    "update:user-role": "tsx scripts/updateUserRole.ts",
    "import:square": "tsx scripts/importSquareData.ts",
    "import:square:enhanced": "tsx scripts/importSquareDataEnhanced.ts",
    "import:payments:manual": "tsx scripts/importManualPayments.ts",
    "update:cloudinary-artist": "tsx scripts/updateCloudinaryArtist.ts",
    "sync:square-bookings": "tsx scripts/syncSquareBookings.ts",
    "create:square-test-data": "tsx scripts/createSquareTestData.ts",
    "diagnose:db": "tsx scripts/diagnose-db-connection.ts",
    "diagnose:square": "tsx scripts/diagnoseSquareBookings.ts",
    "test:square-payments": "tsx scripts/verifySquarePayments.ts",
    "verify:email": "tsx scripts/verify-email-system.ts",
    "test:appointment-email": "tsx scripts/test-appointment-email.ts",
    "checklist": "tsx scripts/pre-launch-checklist.ts"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/multipart": "^8.0.0",
    "@hookform/resolvers": "^5.0.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tabler/icons-react": "^3.34.1",
    "@types/node-cron": "^3.0.11",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.9.0",
    "chalk": "^5.4.1",
    "chart.js": "^4.5.0",
    "cloudinary": "^2.6.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.0.0",
    "framer-motion": "^12.11.0",
    "lucide-react": "^0.510.0",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "next-cloudinary": "^6.16.0",
    "node-cron": "^4.2.1",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "resend": "^4.7.0",
    "sharp": "0.33.5",
    "square": "^42.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.3",
    "@prisma/client": "^6.16.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/colors": "^1.1.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/ui": "^2.1.9",
    "colors": "^1.4.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "daisyui": "^4.12.12",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "happy-dom": "^17.5.6",
    "i": "^0.3.7",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.8.4",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.16.1",
    "sharp-cli": "^5.2.0",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.15",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5",
    "undici": "^7.10.0",
    "vitest": "^2.1.9",
    "wait-on": "^8.0.3",
    "whatwg-fetch": "^3.6.20"
  }
}
