{
  "buildCommand": "npm run build:vercel",
  "installCommand": "npm run clean:vercel && npm install",
  "framework": "nextjs",
  "functions": {
    "src/app/**/*.tsx": {
      "maxDuration": 30
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "FORCE_REBUILD": "v11-fix-next-modules",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "crons": []
}