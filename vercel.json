{
  "buildCommand": "rm -rf .next && npm run build",
  "framework": "nextjs",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": ".next",
  "functions": {
    "src/app/dashboard/tattoo-request/[id]/page.tsx": {
      "maxDuration": 30
    },
    "src/app/dashboard/appointments/[id]/page.tsx": {
      "maxDuration": 30
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/tattooRequest/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "NEXT_TELEMETRY_DISABLED": "1",
      "FORCE_REBUILD": "v4-fix-next15-module-resolution"
    }
  },
  "github": {
    "silent": true
  }
}