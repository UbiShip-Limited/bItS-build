{
  "buildCommand": "rm -rf .next node_modules/.cache && npm run build",
  "framework": "nextjs",
  "installCommand": "npm ci --force",
  "outputDirectory": ".next",
  "functions": {
    "src/app/**/*.tsx": {
      "maxDuration": 30
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "NEXT_TELEMETRY_DISABLED": "1",
      "FORCE_REBUILD": "v5-fix-launcher-module-resolution",
      "NEXT_PRIVATE_STANDALONE": "false"
    }
  },
  "github": {
    "silent": true
  }
}