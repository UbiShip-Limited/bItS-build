{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "functions": {
    "src/app/dashboard/tattoo-request/[id]/page.tsx": {
      "maxDuration": 30
    },
    "src/app/dashboard/appointments/[id]/page.tsx": {
      "maxDuration": 30
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096"
  }
}